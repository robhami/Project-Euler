function multiplesOf3and5(number) {

  var answer=0;
  //loop through all numbers below parameter value
  for (var i=0; i<number; i++){
	  //if divides by 3 or 5 with no remainder
    if (i%5===0 || i%3===0){
    //sum all numbers 
        answer=answer+i;
   
    }
  }
  console.log(answer);
  return answer;
  
}

multiplesOf3and5(19564);

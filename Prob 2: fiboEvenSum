function fiboEvenSum(n) {
  // You can do it!
let fib =0;
let array =[1,2]; 
let answer = 0;
  for (let i=1;i<n-1;i++){

    let num=(array[i])
    let prevNum=(array[i-1]);
    let fib=(num+prevNum);
    array.push(fib);
    if (fib%2===0){
      answer = answer+fib;
    }

  }

answer=answer+2;
console.log(answer);

  return answer;
}

fiboEvenSum(43);
